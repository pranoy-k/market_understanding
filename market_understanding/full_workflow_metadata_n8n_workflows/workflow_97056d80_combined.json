        },
        "typeVersion": 1
      },
      {
        "name": "git commit",
        "type": "n8n-nodes-base.executeCommand",
        "position": [
          900,
          300
        ],
        "parameters": {
          "command": "=git -C repo commit -m \"Auto backup ({{ new Date().toISOString() }})\""
