              "investors": "Investors",
              "company_name": "Company Name",
              "announced_date": "Announced Date",
              "crunchbase_url": "Crunchbase URL",
              "money_raised_usd": "Money Raised (USD)",
              "funding_round_type": "Funding Round Type"
            },
            "mappingMode": "autoMapInputData"
          },
          "options": {},
          "operation": "append",
          "sheetName": {
            "__rl": true,
            "mode": "list",
            "value": "gid=0"
          },
          "documentId": {
