        ],
        "parameters": {
          "url": "https://api.crunchbase.com/api/v4/entities/funding_rounds",
          "options": {},
          "sendQuery": true,
          "sendHeaders": true,
          "queryParameters": {
            "parameters": [
              {
                "name": "locations",
                "value": "United States"
              },
