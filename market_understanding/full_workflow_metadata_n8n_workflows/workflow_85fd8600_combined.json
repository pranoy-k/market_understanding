      {
        "id": "a378c107-d3f5-43cd-bc8c-ad9a39a9ec60",
        "name": "Threads Post",
        "type": "n8n-nodes-base.code",
        "position": [
          800,
          100
        ],
        "parameters": {
          "jsCode": "// Á¢∫‰øù items ÊòØÂê¶ÊúâÂÖßÂÆπ\nif (!items || items.length === 0) {\n  console.log('No items found');\n  return [];\n}\n\n// ÂèñÂæóÊâÄÊúâË≤ºÊñá\nconst posts = items.map(item => item.json).flat();\nconsole.log('Number of posts:', posts.length);\n\n// ÂèñÂæóÁ¨¨‰∏ÄÁØáË≤ºÊñáÁöÑÂü∫Êú¨Ë≥áË®ä\nconst firstPost = posts[0] || {};\n\n// ÁîüÊàê blocks ÁµêÊßã\nconst blocks = [];\n\n// ËôïÁêÜÊØèÂÄãË≤ºÊñá\nposts.forEach(post => {\n  // Â¶ÇÊûúÊúâÊñáÂ≠óÔºåÂä†ÂÖ•ÊñáÂ≠óÂçÄÂ°ä\n  if (post.text) {\n    blocks.push({\n      object: \"block\",\n      type: \"paragraph\",\n      paragraph: {\n        rich_text: [{\n          type: \"text\",\n          text: { content: post.text }\n        }]\n      }\n    });\n  }\n  \n  // Â¶ÇÊûúÊúâÂ™íÈ´îÈÄ£ÁµêÔºåÂä†ÂÖ• embed ÂçÄÂ°ä\n  if (post.media_urls && post.media_urls.length > 0) {\n    post.media_urls.forEach(url => {\n      blocks.push({\n        object: \"block\",\n        type: \"embed\",\n        embed: { url }\n      });\n    });\n  }\n});\n\n// Âêà‰ΩµÂü∫Êú¨Ë≥áË®äÂíå blocks\nconst combinedPost = {\n  id: firstPost.id || '',\n  permalink: firstPost.permalink || '',\n  username: firstPost.username || '',\n  timestamp: firstPost.timestamp || '',\n  blocks\n};\n\n// Â∞áÁµêÊûúËΩâÊèõÁÇ∫ n8n ÊâÄÈúÄÊ†ºÂºè\nreturn [{ json: combinedPost }];"
        },
        "typeVersion": 2
      },
