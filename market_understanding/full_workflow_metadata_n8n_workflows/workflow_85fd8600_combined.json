        ],
        "parameters": {
          "jsCode": "// Á¢∫‰øù items ÊòØÂê¶ÊúâÂÖßÂÆπ\nif (!items || items.length === 0) {\n  console.log('No items found');\n  return [];\n}\n\n// ÂèñÂæóËº∏ÂÖ•Êï∏Êìö\nconst inputData = items[0].json;\nconsole.log('Input data:', JSON.stringify(inputData, null, 2));\n\nif (!inputData || !inputData.data || !Array.isArray(inputData.data)) {\n  console.log('Invalid data structure');\n  return [];\n}\n\n// ÈÅéÊøæÂá∫ username ÁÇ∫ yourThreadsName ÁöÑË≥áÊñô\nconst filteredPosts = inputData.data.filter(post => post.username === 'geekaz');\nconsole.log('Filtered posts count:', filteredPosts.length);\n\n// ËôïÁêÜÊØèÂÄã postÔºåÊèêÂèñÊâÄÈúÄÁöÑË≥áÊñô\nconst processedData = filteredPosts.map(post => {\n  // ÂàùÂßãÂåñ mediaUrlsÔºåÁî®‰æÜÂ≠òÊîæÊâÄÊúâÁöÑ media_url\n  let mediaUrls = [];\n\n  // Â¶ÇÊûúÊúâ childrenÔºåÂâáÊèêÂèñ children Ë£°ÁöÑ media_url\n  if (post.children?.data && Array.isArray(post.children.data)) {\n    mediaUrls = post.children.data\n      .map(child => child.media_url) // ÊèêÂèñÊØèÂÄã child ÁöÑ media_url\n      .filter(url => url); // ÈÅéÊøæÊéâ undefined Êàñ null ÁöÑ URL\n  } else if (post.media_url) {\n    // Â¶ÇÊûúÊ≤íÊúâ childrenÔºå‰ΩøÁî®ÊúÄÂ§ñÂ±§ÁöÑ media_url\n    mediaUrls.push(post.media_url);\n  }\n\n  // ËøîÂõûÊØèÂÄã post ÁöÑËôïÁêÜÂæåÁµêÊûú\n  return {\n    text: post.text || '',\n    media_urls: mediaUrls\n  };\n});\n\nconsole.log('Processed data:', JSON.stringify(processedData, null, 2));\n\n// Â∞áÁµêÊûúËΩâÊèõÁÇ∫ n8n ÊâÄÈúÄÊ†ºÂºè\nreturn processedData.map(post => ({ json: post }));"
        },
        "typeVersion": 2
      },
      {
        "id": "c0cae676-acff-493e-b957-26df0366cf98",
