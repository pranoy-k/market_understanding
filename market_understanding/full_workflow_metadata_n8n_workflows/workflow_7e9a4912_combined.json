          "operation": "price",
          "currencies": [
            "eur"
          ]
        },
        "typeVersion": 1
      },
      {
        "name": "IF",
        "type": "n8n-nodes-base.if",
        "position": [
          990,
          500
        ],
        "parameters": {
          "conditions": {
            "number": [
              {
                "value1": "={{$node[\"CoinGecko\"].json[\"bitcoin\"][\"eur\"]}}",
                "value2": 9000,
                "operation": "largerEqual"
              }
            ]
          }
        },
        "typeVersion": 1
      },
      {
