            }
          ]
        ]
      },
      "Google Calendar": {
        "main": [
          [
            {
              "node": "IF Zoom meeting",
              "type": "main",
              "index": 0
            }
          ]
        ]
      },
      "IF Zoom meeting": {
        "main": [
          [
            {
              "node": "Zoom",
              "type": "main",
              "index": 0
            }
          ]
        ]
      },
      "On clicking 'execute'": {
        "main": [
          [
            {
              "node": "Date & Time",
              "type": "main",
              "index": 0
            }
          ]
        ]
      }
    }
